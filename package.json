{
  "name": "lofi-player",
  "version": "1.0.0-beta",
  "private": true,
  "author": "Stanley Owen <stanleyowen06@gmail.com>",
  "dependencies": {
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.1.1",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "clean-css-cli": "^5.4.2",
    "firebase": "^9.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2",
    "react-scripts": "^4.0.3",
    "sass": "^1.43.4",
    "web-vitals": "^2.1.2"
  },
  "homepage": "./",
  "scripts": {
    "tauri": "tauri",
    "sass": "sass src/App.scss src/App.css",
    "sass-watch": "sass --watch src/App.scss src/App.css",
    "start": "yarn sass && react-scripts start",
    "build": "yarn sass && yarn css:minify && GENERATE_SOURCEMAP=false react-scripts build",
    "build:win": "yarn sass && yarn css:minify && cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "css:minify": "cleancss -o ./src/App.min.css ./src/App.css",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint -c .eslintrc.yml --ext .tsx ./src"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^8.2.0",
    "eslint-plugin-react": "^7.27.0",
    "prettier": "2.4.1",
    "typescript": "^4.4.4"
  }
}
